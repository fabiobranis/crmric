						aColsAGA[nCount][nPosPais]		:= M->U5_PAIS						
					EndIf
				Next
				lExecutado := .T.
			EndIf
		EndIf
	EndIf
     	
	SU5->(RestArea(aAreaSU5))
	AGA->(RestArea(aAreaAGA))
Return lExecutado

