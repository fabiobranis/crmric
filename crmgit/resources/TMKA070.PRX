Local aButadic	:= {}									 // Array de apoio para o ponto de entrada TK070ABOT.
Local aButtons  := {{"GEOROTA",{|| fGeoWizard("SU5",M->U5_CODCONT,{"M->U5_END","M->U5_MUN","M->U5_EST","M->U5_CEP","M->U5_BAIRRO"},{|lContinua| CodGeoRev(aCoor,NIL,@lContinua)[6] <> M->U5_CEP })},STR0028, STR0029 }}

Default cAlias	:= "SU5"
