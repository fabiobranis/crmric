Function Tk070TudOk(nOpc)

Local aArea     := GetArea()
Local lRet      := .T.
Local lContinua := FindFunction("U_TKVINCU5")			 // P.E. Para validar a inclusao de um contato

If INCLUI
	DbSelectArea("SU5")
	DbSetOrder(1)
	If DbSeek(xFilial("SU5") + M->U5_CODCONT)
		Help( " ", 1, "EXISTCONT")
