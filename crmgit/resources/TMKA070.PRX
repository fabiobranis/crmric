Static Function AjustaSX3()

Local aArea := GetArea() 

If cPaisLoc=="BRA"
	DbSelectArea("SX3")
	dbSetOrder(2)
	If DbSeek("U5_CEP")
		If (SX3->X3_VALID <> 'Tk070Cep()')
			RecLock("SX3",.F.)
			SX3->X3_VALID := 'Tk070Cep()'
			MsUnlock()
		EndIf
	EndIf
	If DbSeek("U5_CPF")
		If (SX3->X3_VALID <> 'Vazio() .OR. ( CGC(M->U5_CPF) .AND. Existchav("SU5",M->U5_CPF,8,"JAGRAVADO") .AND. FreeForUse("SU5",M->U5_CPF) )')
			RecLock("SX3",.F.)
			SX3->X3_VALID := 'Vazio() .OR. ( CGC(M->U5_CPF) .AND. Existchav("SU5",M->U5_CPF,8,"JAGRAVADO") .AND. FreeForUse("SU5",M->U5_CPF) )'
			MsUnlock()
		EndIf
	EndIf
EndIf    

DbSelectArea("SX3")
DbSetOrder(2)
If DbSeek("U5_NOPERAD") .And. AllTrim(X3_RELACAO) <> 'IIF(!INCLUI,Posicione("SU7",1,xFilial("SU7")+SU5->U5_OPERADO,"U7_NOME"),"")'
	RecLock("SX3", .F.)
		REPLACE X3_RELACAO With 'IIF(!INCLUI,Posicione("SU7",1,xFilial("SU7")+SU5->U5_OPERADO,"U7_NOME"),"")'
	MsUnlock()
EndIf

If DbSeek("U5_CODCONT") .And. FindFunction("NewNumCont") .And. AllTrim(X3_RELACAO) <> 'IIF(INCLUI,NewNumCont(),"")'
	RecLock("SX3", .F.)
		REPLACE X3_RELACAO With 'IIF(INCLUI,NewNumCont(),"")'
	MsUnlock()
EndIf      

If DbSeek("U5_CODCONT")
	If( SX3->X3_VALID <> 'IIF(INCLUI,Tk070Cod(M->U5_CODCONT),.T.)' )
		RecLock("SX3",.F.)
		SX3->X3_VALID := 'IIF(INCLUI,Tk070Cod(M->U5_CODCONT),.T.)'		
		MsUnlock()
	EndIf
EndIf 

If cPaisLoc <> "BRA"
	If DbSeek("U5_CODMUN")
		If( SX3->X3_USADO <> X3_NAOUSADO)
			RecLock("SX3",.F.)
			SX3->X3_USADO := X3_NAOUSADO		
			MsUnlock()
		EndIf             		
	EndIf                 
EndIf
	
If __lPyme
	DbSelectArea("SX3")
	DbSetOrder(2)
	If DbSeek("U5_DEPTO") .And. AllTrim(X3_PYME) <> 'N'
		RecLock("SX3", .F.)
		SX3->X3_PYME := 'N'
		MsUnlock()
	EndIf
	If DbSeek("U5_DDEPTO") .And. AllTrim(X3_PYME) <> 'N'
		RecLock("SX3", .F.)
		SX3->X3_PYME := 'N'
		MsUnlock()
	EndIf
EndIf

If SX3->( dbSeek("HU5_CEL"))
	RecLock("SX3",.F.)
	If !Empty(SX3->X3_PICTURE)
   		SX3->X3_PICTURE	:= ""  
   	EndIf 
	MsUnlock()  
EndIf	                 
  
 	
If SX3->( dbSeek("U5_CELULAR"))
	RecLock("SX3",.F.)
	If !Empty(SX3->X3_PICTURE)
   		SX3->X3_PICTURE	:= ""   
   	EndIf  
	MsUnlock()  
EndIf	

If SX3->( dbSeek("AGB_TELEFO"))
	RecLock("SX3",.F.)
	If !Empty(SX3->X3_PICTURE)
   		SX3->X3_PICTURE	:= "" 
   	EndIf  
	MsUnlock()  
EndIf         

If cPaisLoc == "MEX"              
	If SX3->( dbSeek("U5_BAIRRO")) .And. AllTRim((SX3->X3_TITSPA))<> "Colonia"
		RecLock("SX3",.F.)
	   		SX3->X3_TITSPA	:= "Colonia"  
		MsUnlock()  
	EndIf       
	If SX3->( dbSeek("AGA_BAIRRO")) .And. AllTRim((SX3->X3_TITSPA))<> "Colonia"
		RecLock("SX3",.F.)
	   		SX3->X3_TITSPA	:= "Colonia"  
		MsUnlock()  
	EndIf                   

EndIf                  
RestArea(aArea)

Return Nil

